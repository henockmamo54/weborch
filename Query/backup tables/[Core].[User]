CREATE TABLE [Core].[User](
	[ID] [int] IDENTITY(1,1) NOT NULL,
	[EnglishName] [varchar](200) NULL,
	[KoreanName] [nvarchar](200) NULL,
	[UserID] [nvarchar](100) NULL,
