	[Email] [nvarchar](100) NOT NULL,
	[Address] [nvarchar](100) NOT NULL,
	[ZipCode] [nvarchar](10) NULL,
	[sex] [nvarchar](10) NULL,
	[PhoneNumber] [nvarchar](20) NULL,
	[Birthday] [datetime] NULL,
	[Category] [nvarchar](50) NULL,
